#include <iostream>
#include <fstream>
#include <string>

using namespace std;

int main(int argc, char* argv[]) {
    string str;
    ifstream in(argv[1]);
    int i;
    for(i=0; i<10; i++) {
        in >> str;
        if(in.eof())
            break;
        cout << str;
    }
    cout << i;
    return 0;
}
